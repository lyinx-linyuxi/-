page{
  background: #ffc3c3;
}
.m-header{
  background: #ffc3c3;
}
.i-cell{
  background: #ffc3c3;
}
.card-panel{
  padding: 10px;
}
.card-add{
  background-color:#fff;
  overflow: hidden;
  padding: 20px 10px;
  border-radius: 5px;
  box-shadow:0 0 10px rgba(0, 0, 0, 0.1);
  position: relative;
}
.card-add .title{
  font-size: 16px;
}
.card-add .desc{
  font-size: 12px;
  color: #999;
}
.card-add .image{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 140px;
  height: 52px;
}
.card-add .image image{
  width: 100%;
  height: 100%;
  display: block;
}